<h2>Show frame photos</h2>

<ul>

  <li>
    <strong>Frame:</strong>
    <%= @frame_photos.frame_id %>
  </li>

  <li>
    <strong>Photo url:</strong>
    <img src="<%= "http://localhost:4000" <> photo_url(@frame_photos.photo_url) %>" alt="">
  </li>

</ul>

<%= link "Edit", to: frame_photos_path(@conn, :edit, @frame_photos) %>
<%= link "Back", to: frame_photos_path(@conn, :index) %>
